 Particle {
     list = [ "dark" ];
     mass_is_mass = true;
     dark {
         attributes = [ "x", "default",
	 	        "y", "default",
			"z", "default",
			"vx", "default",
			"vy", "default",
			"vz", "default",
			"ax", "default",
			"ay", "default",
			"az", "default" ,
                        "is_local", "default"];
         position = [ "x", "y", "z" ];
	 velocity = [ "vx", "vy", "vz" ];
         constants = [ "mass", "default", 2.5581226e-5 ];
         group_list = [ "is_gravitating" ];
     }
 }

 Physics {
     list = [ "cosmology", "gravity" ];
     cosmology {
        omega_baryon_now    = 0.0;
        omega_cdm_now       = 0.305;
        omega_matter_now    = 0.305;
        omega_lambda_now    = 0.695;
        hubble_constant_now = 0.6777;
        max_expansion_rate  = 0.015;
        initial_redshift    = 50.0;
        final_redshift      = 0.0;
	comoving_box_size   = 100.0;
     }

     # we explicitly avoid setting the gravitational constant since this is a
     # cosmological simulation
 }

 Stopping {
     cycle = 100000;
     redshift = 0.0;
 }

 
