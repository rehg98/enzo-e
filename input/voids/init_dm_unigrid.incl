 Adapt {
     SLOPE {
         field_list = [ "density" ];
         max_coarsen = 2.0;
         min_refine = 5.0;
         type = "slope";
     };
     list = [ "SLOPE" ];
     max_level = 0;
 }

 Boundary {
     type = "periodic";
 }

 Domain {
     lower = [ 0.0, 0.0, 0.0 ];
     upper = [ 1.0, 1.0, 1.0 ];
 }

 Field {
     history = 1;
     alignment = 8;
     gamma = 1.6667;
     ghost_depth = 3;
     list = [ "density",
     	      "velocity_x", "velocity_y", "velocity_z",
     	      "acceleration_x", "acceleration_y", "acceleration_z",
	      "total_energy", "internal_energy", "pressure",
     	  "density_total", "X", "B", "potential", "density_gas",
	       "density_particle", "density_particle_accumulate"];
     padding = 0;
 }

 Initial {

     # "new" initialization required for "hdf5" Initial type
     # (supports communication, but currently limited to unigrid)

     new = true;
     
     list = [ "hdf5", "cosmology" ];
     hdf5 {
         format = "music";

         PVX {
             attribute = "vx";
             dataset = "ParticleVelocities_x";
             file = "input/voids/void_ics_dm_uni_music2.enzo/ParticleVelocities_x";
             name = "dark";
             type = "particle";
         };
         PVY {
             attribute = "vy";
             dataset = "ParticleVelocities_y";
             file = "input/voids/void_ics_dm_uni_music2.enzo/ParticleVelocities_y";
             name = "dark";
             type = "particle";
         };
         PVZ {
             attribute = "vz";
             dataset = "ParticleVelocities_z";
             file = "input/voids/void_ics_dm_uni_music2.enzo/ParticleVelocities_z";
             name = "dark";
             type = "particle";
         };
         PX {
             attribute = "x";
             dataset = "ParticleDisplacements_x";
             file = "input/voids/void_ics_dm_uni_music2.enzo/ParticleDisplacements_x";
             name = "dark";
             type = "particle";
         };
         PY {
             attribute = "y";
             dataset = "ParticleDisplacements_y";
             file = "input/voids/void_ics_dm_uni_music2.enzo/ParticleDisplacements_y";
             name = "dark";
             type = "particle";
         };
         PZ {
             attribute = "z";
             dataset = "ParticleDisplacements_z";
             file = "input/voids/void_ics_dm_uni_music2.enzo/ParticleDisplacements_z";
             name = "dark";
             type = "particle";
         };
         file_list = [ "PX", "PY", "PZ", "PVX", "PVY", "PVZ" ];
     }
 }

 Mesh {
     root_rank = 3;
     root_size = [ 128, 128, 128 ];
 }

 Method {
     list = [ "comoving_expansion",
              "cosmology",
              "pm_deposit",
	      "gravity",
	      "pm_update"
	      ];
     gravity {
     	solver = "cg";
    }
 }

 Solver {
    list = ["cg"];
    cg {
       type = "cg";
       iter_max = 100;
       res_tol = 1e-6;
       monitor_iter = 10;
    }
 } 
 Output {
     # list = [ "de", "vx", "vy", "vz", "ax", "ay", "az", "po", "dt", "dark", "hdf5" ];
     list = [ "dark", "hdf5" ];
 
